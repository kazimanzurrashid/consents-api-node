{
  "name": "consents-api",
  "version": "0.1.0",
  "private": true,
  "main": "build/app.js",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "start": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.test.ts\" --exec \"node_modules/.bin/ts-node src/app.ts",
    "compile": "tsc --noEmit",
    "format": "prettier --write **/*.ts",
    "lint": "eslint ./ --fix",
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "rimraf build && tsc",
    "pack": "rimraf dist && ncc build --source-map",
    "release": "npm run format && npm run lint && npm run coverage && npm run build && npm run pack"
  },
  "dependencies": {
    "express": "^4.18.1",
    "express-pino-logger": "^7.0.0",
    "joi": "^17.6.0",
    "pg": "^8.8.0",
    "pino": "^8.4.2",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "@types/pg": "^8.6.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.36.1",
    "@vercel/ncc": "^0.34.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
