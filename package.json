{
  "name": "consents-api",
  "version": "0.1.0",
  "private": true,
  "main": "build/src/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kazimanzurrashid/consents-api-node"
  },
  "scripts": {
    "prepare": "husky install",
    "compile": "tsc --noEmit",
    "format": "prettier --write ./src/**/*",
    "lint": "eslint ./ --fix",
    "lint-staged": "lint-staged",
    "commit-lint": "commitlint",
    "test": "jest",
    "coverage": "jest --ci --coverage",
    "build": "rimraf build && tsc",
    "pack": "rimraf dist && ncc build --source-map",
    "copy-other-required-files": "ts-node copy-other-required-files.ts",
    "release": "release-it",
    "start": "nodemon src/index.ts"
  },
  "dependencies": {
    "express": "^4.18.1",
    "express-pino-logger": "^7.0.0",
    "joi": "^17.6.2",
    "pg": "^8.8.0",
    "pino": "^8.6.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.5.0",
    "tsyringe": "^4.7.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@faker-js/faker": "^7.5.0",
    "@release-it/conventional-changelog": "^5.1.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.3",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vercel/ncc": "^0.34.0",
    "copy-dir": "^1.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^27.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "release-it": "^15.5.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.0",
    "testcontainers": "^8.15.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
